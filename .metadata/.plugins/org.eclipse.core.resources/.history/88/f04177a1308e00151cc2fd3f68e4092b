
public class DisplayWeight {
	public static void main(String[] args) {

		int WeightOfPerson = 150;
		int WeightLimit =450;
		int PersonLimit;
		
		PersonLimit = WeightLimit/WeightOfPerson;
		
		System.out.print("Max");
		System.out.print(" number of people = ");
		System.out.println(PersonLimit);

			}
}
